<%- include('../includes/head.ejs') %>
    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>
            <main>
                <% if(orders.length <=0) { %>
                    <h1>Nothibg there!</h1>
                    <% } else {%>
                        <ul>
                            <% orders.forEach(order=> { %>
                                <li>
                                    <h1># <%= order.id %>
                                    </h1>
                                    <ul>
                                        <% order.products.forEach(item=> { %>
                                            <li>
                                                <%= item.title %> x <%= item.orderItems.quantity %>
                                            </li>
                                            <% }); %>
                                    </ul>
                                </li>
                                <% }); %>
                        </ul>
                        <% } %>
            </main>
            <%- include('../includes/end.ejs') %>